<div class = "container">
	<%= form_for(:session, url: sessions_path) do |f| %>
	  <%= f.text_field :email, :placeholder => "Insert Email Here"%>
	    <br>
	  <%= f.password_field :password, :placeholder => "Password"%>
		<br>
	  <%= f.submit "Sign In!", class: "btn btn-large btn-primary" %>
	<% end %>
</div>
