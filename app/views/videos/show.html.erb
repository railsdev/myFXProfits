<% if @h264.status == "success" %>
  <video id="movie" width="<%= @h264.width %>" height="<%= @h264.height %>" preload="none"
    poster="<%= @h264.screenshots.first %>" controls
    <source ="<%= @h264.url %>" type="video/mp4">
  </video>
<% else %>
   Your video is processing: <%= @h264.encoding_progress.to_i %>%
<% end %>